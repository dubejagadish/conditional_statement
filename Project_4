#include<stdio.h>

int main()
{
    int a;
    printf("Enter the number:");
    scanf("%d",&a);
    
        if (a==1)
        {printf("1 Pizza, Rs 239");}
       
        else if (a==2)
        {printf("2 Burger, Rs 129");}
        
        else if (a==3)
        {printf("3  Pasta, Rs 179");}
        
        else if (a==4)
        {printf("4 French fries, Rs 99");}
    
        else if (a==5)
    
        {printf("5 Sandwich, Rs 149");}
        
        else
        {printf("Please enter valid number");}
       
        
    
return(0);
}
